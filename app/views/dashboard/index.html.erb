<h1>Listing chefs</h1>

  <table>
    <tr>
      <th>Id</th>
      <th>Name</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Branchef</th>
    </tr>

    <% @chef.each do |c| %>
      <tr>
        <td><%= c.id %></td>
        <td><%= c.name %></td>
        <td><%= c.email %></td>
        <td><%= c.phone %></td>
        <td><%= c.is_branchef %></td>
        <td><%= link_to 'Edit', edit_chef_registration_path(c) %></td>
      </tr>
    <% end %>
  </table>
